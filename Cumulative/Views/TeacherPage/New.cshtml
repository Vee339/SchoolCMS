<h1>Fill the information to add new teacher:</h1>

<form method="POST" action="/TeacherPage/Create">
  <div class="inputBox">
      <label for="firstName">First Name:</label>
      <input type="text" id="firstName" name="first_name"/>
  </div>
    <div class="inputBox">
        <label for="lastName">Last Name:</label>
        <input type="text" id="lastName" name="last_name" />
    </div>
    <div class="inputBox">
      <label for="employeeNum">Employee Number:</label>
      <input type="text" id="employeeNum" name="employee_number"/>
  </div>
  <div class="inputBox">
      <label for="hireDate">Hire Date:</label>
      <input type="date" id="hireDate" name="hire_date"/>
  </div>
  <div class="inputBox">
        <label for="salary">Salary:</label>
        <input type="text" id="salary" name="salary"/>
  </div>
  <input type="submit" class="btn" value="Add Teacher"/>
</form>